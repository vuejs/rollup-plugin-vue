{
  "name": "rollup-plugin-vue",
  "version": "4.1.2",
  "description": "Roll .vue files",
  "author": "Rahul Kadyan <hi@znck.me>",
  "bugs": {
    "url": "https://github.com/znck/rollup-plugin-vue/issues"
  },
  "homepage": "https://github.com/znck/rollup-plugin-vue#readme",
  "keywords": [
    "rollup-plugin",
    "vue"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/znck/rollup-plugin-vue.git"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "yarn test && yarn docs && git add docs/"
    }
  },
  "scripts": {
    "prepublishOnly": "yarn build",
    "prebuild": "yarn lint && yarn test",
    "build": "tsc",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "docs": "typedoc typings src/index.ts",
    "lint": "prettier --no-semi --single-quote --write **/*.js **/*.vue !test/target/** !dist/**",
    "release": "standard-version -a",
    "test": "jest"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.46",
    "@vue/component-compiler": "^3.3.1",
    "@vue/component-compiler-utils": "^1.2.1",
    "debug": "^2.6.0",
    "hash-sum": "^1.0.2",
    "querystring": "^0.2.0",
    "rollup-pluginutils": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.4",
    "@types/puppeteer": "^1.3.1",
    "@znck/promised": "^1.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^22.4.3",
    "conventional-changelog": "^1.1.24",
    "jest": "^22.4.2",
    "node-sass": "^4.9.0",
    "prettier": "^1.12.1",
    "pug": "^2.0.3",
    "puppeteer": "^1.3.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-image": "^1.0.2",
    "rollup-plugin-md": "^0.0.7",
    "rollup-plugin-node-resolve": "^3.3.0",
    "ts-jest": "^22.4.5",
    "typescript": "^2.8.3",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16"
  },
  "peerDependencies": {
    "vue-template-compiler": "*"
  }
}
